<div class="page-header">
<h1>Reports <small>Details for a report</small></h1>
</div>

<h2>Summary</h2>

<table class="table">
  <tr>
    <th scope="row">Status</th>
    <td><%= color_status @report.status %></td>
  </tr>
  <tr>
    <th scope="row">Parser</th>
    <td><%= color_parser_status @report.parser_status %></td>
  </tr>
  <tr>
    <th scope="row">Started</th>
    <td><%= l @report.starttime%></td>
  </tr>
  <tr>
    <th scope="row">Finished</th>
    <% if @report.endtime %>
      <td><%= l @report.endtime %></td>
    <% else %>
      <td>&nbsp;</td>
    <% end %>
  </tr>
  <tr>
    <th scope="row">Submitted by</td>
    <% if current_user.admin? %>
      <td><%= link_to @report.user.name, @report.user %></td>
    <% else %>
      <td><%= @report.user.name %></td>
    <% end %>
  </tr>
</table>

<p><%= link_to 'Back', reports_path %><p>

<h2>Attached data</h2>

<%= render 'data' %>
