<div class="page-header">
  <h1>Revisions <small>Overview</small></h1>
</div>

<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Asset</th>
      <th>Date</th>
      <th>&nbsp;</th>
    </tr>
  </thead>

  <tbody>
    <% @revisions.each do |revision| %>
      <tr>
        <td><%= link_to revision.revisionable.name, revision.revisionable %></td>
        <td><%= revision.created_at %></td>
        <td><%= link_to 'Show', revision %></td>
      </tr>
    <% end %>
  </tbody>
</table>
